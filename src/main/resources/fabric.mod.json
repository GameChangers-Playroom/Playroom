{
	"schemaVersion": 1,
	"id": "playroom",
	"version": "${version}",
	"name": "Playroom (2024)",
	"description": "The official mod for the Playroom 2024 charity event!",
	"authors": [
		"FlameyHeart (Organizer)",
		"Awakened Redstone (Developer)",
		"Bawnorton (Developer)",
		"MacStevins (Developer)",
		"BlazeFaia (Artist)",
		"NBeeMC (Artist)"
	],
	"license": "MIT",
	"icon": "assets/playroom/icon.png",
	"environment": "*",
